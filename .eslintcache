        jokes.push({ id: uuid(), text: newJoke, votes: 0 })\r\n            } else {\r\n                console.log(\"FOUND A DUPLICATE!\");\r\n                console.log(newJoke)\r\n            }\r\n        }\r\n\r\n        this.setState(st => ({\r\n            loading: false,\r\n            jokes: [...st.jokes, ...jokes]\r\n        }), () => window.localStorage.setItem(\"jokes\", JSON.stringify(jokes)))\r\n\r\n        window.localStorage.setItem(\"jokes\", JSON.stringify(jokes));\r\n        } catch(e) {\r\n            alert(e)\r\n            this.setState({ loading: false })\r\n        }\r\n    }\r\n\r\n    handleClick() {\r\n        this.setState({ loading: true }, this.getJokes)        \r\n    }\r\n\r\n    componentDidMount() {\r\n        if(this.state.jokes.length === 0) this.getJokes();\r\n    }\r\n\r\n    render() {\r\n        if(this.state.loading) {\r\n            return (\r\n                <div className=\"Jokelist-spinner\">\r\n                    <i className=\"far fa-8x fa-laugh fa-spin\" />\r\n                    <h1 className=\"Jokelist-title\">Loading...</h1>\r\n                </div>\r\n            )\r\n        }\r\n\r\n        let jokes = this.state.jokes.sort((a, b) => b.votes - a.votes)\r\n\r\n        return (\r\n            <div className=\"JokeList\">\r\n                <div className=\"JokeList-sidebar\">\r\n                    <h1 className=\"JokeList-title\">\r\n                        <span>Dad</span> Jokes\r\n                    </h1>\r\n                    <img src='https://assets.dryicons.com/uploads/icon/svg/8927/0eb14c71-38f2-433a-bfc8-23d9c99b3647.svg' />\r\n                    <button className='JokeList-getmore' onClick={this.handleClick}>Fetch Jokes</button>\r\n                </div>\r\n                \r\n                <div className=\"JokeList-jokes\">\r\n                    { jokes.map(joke => (\r\n                        <Joke key={joke.id} votes={joke.votes} text={joke.text} upvote={() => this.handleVote(joke.id, 1)} downvote={() => this.handleVote(joke.id, -1)} />\r\n                    )) }\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default JokeList\r\n// 19 - 12","C:\\Users\\rommel.torquator\\Desktop\\hello-world\\src\\components2\\Joke.js",[],"C:\\Users\\rommel.torquator\\Desktop\\hello-world\\src\\components2\\Contact.js",[],"C:\\Users\\rommel.torquator\\Desktop\\hello-world\\src\\components2\\Dog.js",[],"C:\\Users\\rommel.torquator\\Desktop\\hello-world\\src\\components2\\About.js",[],"C:\\Users\\rommel.torquator\\Desktop\\hello-world\\src\\components2\\VendingMachine.js",[],"C:\\Users\\rommel.torquator\\Desktop\\hello-world\\src\\components2\\Chips.js",[],"C:\\Users\\rommel.torquator\\Desktop\\hello-world\\src\\components2\\Sardines.js",[],"C:\\Users\\rommel.torquator\\Desktop\\hello-world\\src\\components2\\Soda.js",[],"C:\\Users\\rommel.torquator\\Desktop\\hello-world\\src\\components2\\Navbar.js",[],"C:\\Users\\rommel.torquator\\Desktop\\hello-world\\src\\components2\\Food.js",[],{"ruleId":"237","replacedBy":"238"},{"ruleId":"239","replacedBy":"240"},{"ruleId":"241","severity":1,"message":"242","line":1,"column":8,"nodeType":"243","messageId":"244","endLine":1,"endColumn":12},{"ruleId":"241","severity":1,"message":"245","line":4,"column":8,"nodeType":"243","messageId":"244","endLine":4,"endColumn":13},{"ruleId":"241","severity":1,"message":"246","line":5,"column":8,"nodeType":"243","messageId":"244","endLine":5,"endColumn":15},{"ruleId":"241","severity":1,"message":"247","line":6,"column":8,"nodeType":"243","messageId":"244","endLine":6,"endColumn":13},{"ruleId":"241","severity":1,"message":"248","line":7,"column":8,"nodeType":"243","messageId":"244","endLine":7,"endColumn":15},{"ruleId":"241","severity":1,"message":"249","line":8,"column":8,"nodeType":"243","messageId":"244","endLine":8,"endColumn":15},{"ruleId":"241","severity":1,"message":"250","line":9,"column":8,"nodeType":"243","messageId":"244","endLine":9,"endColumn":21},